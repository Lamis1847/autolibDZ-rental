<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.view.fragment.splashscreen.SplashFragmentThree"
    android:background="@color/white">

    <ImageView
        android:id="@+id/imageViewCar"
        android:layout_width="wrap_content"
        android:layout_height="294dp"
        android:layout_marginTop="88dp"
        android:src="@drawable/ic_splash2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewSplashThree"
        android:layout_width="334dp"
        android:layout_height="95dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/gothambold"
        android:gravity="center"
        android:text="@string/textViewSplashThree"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewCar" />

    <ImageView
        android:id="@+id/imageViewRound3_S3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:src="@drawable/ic_rond_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.234"
        app:layout_constraintStart_toEndOf="@+id/imageViewRound2_S3"
        app:layout_constraintTop_toBottomOf="@+id/textViewSplashThree" />

    <ImageView
        android:id="@+id/imageViewRound1_S3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="104dp"
        android:layout_marginLeft="104dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/ic_rond_1"
        app:layout_constraintStart_toStartOf="@+id/textViewSplashThree"
        app:layout_constraintTop_toBottomOf="@+id/textViewSplashThree" />

    <ImageView
        android:id="@+id/imageViewRound2_S3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/ic_rond_1"
        app:layout_constraintStart_toEndOf="@+id/imageViewRound1_S3"
        app:layout_constraintTop_toBottomOf="@+id/textViewSplashThree" />

    <TextView
        android:id="@+id/textViewPasserThree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:fontFamily="@font/gothambold"
        android:text="@string/passer"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewRound2_S3"
        app:layout_constraintVertical_bias="0.927" />
</androidx.constraintlayout.widget.ConstraintLayout>